{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "CLI plot default dike",
            "type": "python",
            "request": "launch",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "program": "${workspaceFolder}\\dikesfordummies\\main.py",
            "args": [
                "plot_profile",
                "--outfile",
                "dike_plot.png"
            ],
            "justMyCode": true,
        },
    ]
}